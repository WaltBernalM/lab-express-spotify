<div class="track-container">
  {{#each tracks}}
      <div class="track-name-container">
        <emp>{{name}}</emp>
      </div>

      {{#if preview_url}}
      <div>
        <audio controls>
          <source src="{{preview_url}}" type="audio/mp3">
        </audio>
      </div>
      {{else}}
      <div>
        <p>Preview not available</p>
      </div>
      {{/if}}
  {{/each}}
</div>